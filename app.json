{
  "expo": {
    "name": "Tapijulapa",
    "slug": "Tapijulapa",
    "privacy": "public",
    "platforms": ["ios", "android", "web"],
    "version": "1.0.5",

    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.sectur.tapijulapa",
      "usesAppleSignIn": true,
      "infoPlist": {
        "NSPhotoLibraryUsageDescription": "Permite que la aplicación acceda a tu galería de imágenes para que puedas seleccionar una como foto de perfil"
      }
    },
    "android": {
      "package": "com.sectur.tapijulapa",
      "versionCode": 5,
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyC2BRYkoR_s41JBL6wbmqSvzJzAomrO7-w"
        }
      },
      "permissions": [
        "CAMERA_ROLL",
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE",
        "LOCATION",
        "ACCESS_COARSE_LOCATION",
        "ACCESS_FINE_LOCATION"
      ]
    },
    "description": ""
  }
}
